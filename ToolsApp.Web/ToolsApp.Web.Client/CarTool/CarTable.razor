<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Make</th>
            <th>Model</th>
            <th>Year</th>
            <th>Color</th>
            <th>Price</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var car in cars)
        {
            <CarViewRow car="@car" OnSubmitDeleteCar="SubmitDeleteCar" />
        }
    </tbody>
</table>

@code {
    [Parameter]
    public IEnumerable<ICar> cars { get; set; } = new List<Car>();

    [Parameter]
    public EventCallback<ICar> SubmitDeleteCar { get; set; }

}
